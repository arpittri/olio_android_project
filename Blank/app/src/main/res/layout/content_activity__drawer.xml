<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="trainedge.blank.HomeActivity"
    tools:showIn="@layout/app_bar_activity__drawer">

    <ImageView
        android:id="@+id/ivFacebook"
        android:layout_width="80dp"
        android:layout_height="96dp"
        android:layout_marginBottom="100dp"
        android:layout_marginLeft="53dp"
        android:layout_marginTop="92dp"
        android:paddingEnd="0dp"
        android:paddingRight="8dp"
        app:layout_constraintBottom_toTopOf="@+id/ivInstagram"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.461"
        app:srcCompat="@drawable/fb" />

    <ImageView
        android:id="@+id/ivGoogle"
        android:layout_width="80dp"
        android:layout_height="96dp"
        android:layout_marginBottom="100dp"
        android:layout_marginLeft="102dp"
        android:layout_marginRight="32dp"
        android:layout_marginTop="92dp"
        android:paddingEnd="0dp"
        android:paddingRight="8dp"
        app:layout_constraintBottom_toTopOf="@+id/ivTwitter"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/ivFacebook"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.461"
        app:srcCompat="@drawable/go" />

    <ImageView
        android:id="@+id/ivTwitter"
        android:layout_width="80dp"
        android:layout_height="96dp"
        android:layout_marginBottom="149dp"
        android:layout_marginLeft="102dp"
        android:layout_marginRight="32dp"
        android:cropToPadding="false"
        android:paddingEnd="0dp"
        android:paddingRight="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/ivInstagram"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/tw" />

    <ImageView
        android:id="@+id/ivInstagram"
        android:layout_width="80dp"
        android:layout_height="96dp"
        android:layout_marginBottom="149dp"
        android:layout_marginLeft="53dp"
        android:paddingEnd="0dp"
        android:paddingRight="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@drawable/ins" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="237dp"
        android:layout_height="31dp"
        android:text="Want everything at one plece"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/ivInstagram"
        app:layout_constraintHorizontal_bias="0.503" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go Get Olio!"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

</android.support.constraint.ConstraintLayout>
